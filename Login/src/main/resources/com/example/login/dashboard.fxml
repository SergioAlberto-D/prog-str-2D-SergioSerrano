      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.login.controllers.DashboardController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <Label fx:id="labelNombre" textAlignment="CENTER">
        <font>
            <Font size="20.0" />
        </font>
    </Label>
</VBox>
